{
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node@3.0.0"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build@2.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
