{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "outDir": "./src/dist",
        "rootDir": "./src/scripts",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/scripts/**/*"],
    "exclude": ["node_modules"]
}